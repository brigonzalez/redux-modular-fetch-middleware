{
  "name": "redux-modular-fetch-middleware",
  "version": "1.0.1",
  "description": "A modular redux middleware for using fetch",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && babel ./src --out-dir ./dist",
    "lint": "eslint ./src ./tests",
    "prepublish": "npm run test && npm run build",
    "test": "npm run lint && npm run unit",
    "unit": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brigonzalez/redux-modular-fetch-middleware.git"
  },
  "keywords": [
    "redux",
    "modular",
    "fetch",
    "middleware",
    "action",
    "creator",
    "dispatch"
  ],
  "author": {
    "name": "Brian Gonzalez",
    "email": "developer.briangonzalez@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brigonzalez/redux-modular-fetch-middleware/issues"
  },
  "homepage": "https://github.com/brigonzalez/redux-modular-fetch-middleware#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chance": "^1.0.16",
    "eslint": "^4.19.1",
    "eslint-config-get-off-my-lawn": "^4.0.0",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/.jest-coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
